{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#a2ab70",
          "options": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "style": "plain",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }}<#685d52> | </>",
          "type": "os"
        },
        {
          "foreground": "#d4b888",
          "style": "plain",
          "template": "{{ if ne .Env.POSH_SESSION_DEFAULT_USER .UserName }}<#8ead89>{{ .UserName }}</><#d4b888>@</>{{ end }}<#8ead89>{{ .HostName }}</><#685d52> | </>",
          "type": "session"
        },
        {
          "foreground": "#c7a268",
          "style": "plain",
          "template": "\uf0e7<#685d52> | </>",
          "type": "root"
        },
        {
          "foreground": "#d4b888",
          "options": {
            "style": "full",
            "mapped_locations": {
              "~": "~"
            }
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#c7a268",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#cc8850{{ end }}",
            "{{ if gt .Ahead 0 }}#8ead89{{ end }}",
            "{{ if gt .Behind 0 }}#c990a0{{ end }}"
          ],
          "options": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#685d52>| </>{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#c97060> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#a2ab70> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#a2ab70",
          "options": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#d4b888",
          "style": "plain",
          "template": "~#@\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#c7a268",
          "options": {
            "style": "roundrock",
            "threshold": 500
          },
          "style": "plain",
          "template": " {{ .FormattedMs }}<#d4b888> \uf252 </>",
          "type": "executiontime"
        },
        {
          "foreground": "#a2ab70",
          "foreground_templates": ["{{ if gt .Code 0 }}#c97060{{ end }}"],
          "options": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \uea9f {{ if gt .Code 0 }}{{ .Code }}{{ end }} ",
          "type": "status"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "final_space": true,
  "version": 4
}
