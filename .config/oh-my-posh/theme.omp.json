{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8a9a7b",
          "options": {
            "alpine": "",
            "arch": "",
            "centos": "",
            "debian": "",
            "elementary": "",
            "fedora": "",
            "gentoo": "",
            "linux": "",
            "macos": "",
            "manjaro": "",
            "mint": "",
            "opensuse": "",
            "raspbian": "",
            "ubuntu": "",
            "windows": ""
          },
          "style": "plain",
          "template": " {{ .Icon }}<#a6a69c> | </>",
          "type": "os"
        },
        {
          "foreground": "#c5c9c5",
          "style": "plain",
          "template": "{{ if ne .Env.POSH_SESSION_DEFAULT_USER .UserName }}<#8ba4b1>{{ .UserName }}</><#c4b28a>󰁥</></>{{ end }}<#8ea4a2>{{ .HostName }}</><#a6a69c> | </>",
          "type": "session"
        },
        {
          "foreground": "#c4b28a",
          "style": "plain",
          "template": "󱐋<#a6a69c> | </>",
          "type": "root"
        },
        {
          "foreground": "#c5c9c5",
          "options": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#c4b28a",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4b28a{{ end }}",
            "{{ if gt .Ahead 0 }}#8ea4a2{{ end }}",
            "{{ if gt .Behind 0 }}#a292a3{{ end }}"
          ],
          "options": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#a6a69c>| </>{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#c4746e> 󰤌 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#8a9a7b>  {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#8a9a7b",
          "options": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "  {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "#c4b28a",
          "options": {
            "style": "roundrock",
            "threshold": 500
          },
          "style": "plain",
          "template": "  {{ .FormattedMs }} </>",
          "type": "executiontime"
        },
        {
          "foreground": "#8a9a7b",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#c4746e{{ end }}"
          ],
          "options": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "  {{ if gt .Code 0 }}{{ .Code }}{{ end }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#c5c9c5",
          "style": "plain",
          "template": "$",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "final_space": true,
  "version": 4
}
