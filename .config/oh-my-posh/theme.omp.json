{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#819b69",
          "options": {
            "alpine": "",
            "arch": "",
            "centos": "",
            "debian": "",
            "elementary": "",
            "fedora": "",
            "gentoo": "",
            "linux": "",
            "macos": "",
            "manjaro": "",
            "mint": "",
            "opensuse": "",
            "raspbian": "",
            "ubuntu": "",
            "windows": ""
          },
          "style": "plain",
          "template": " {{ .Icon }}<#3d3839> | </>",
          "type": "os"
        },
        {
          "foreground": "#bbbbbb",
          "style": "plain",
          "template": "{{ if ne .Env.POSH_SESSION_DEFAULT_USER .UserName }}<#6099c0>{{ .UserName }}</><#b77e64>󰁥</></>{{ end }}<#66a5ad>{{ .HostName }}</><#3d3839> | </>",
          "type": "session"
        },
        {
          "foreground": "#b77e64",
          "style": "plain",
          "template": "󱐋<#3d3839> | </>",
          "type": "root"
        },
        {
          "foreground": "#bbbbbb",
          "options": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#b77e64",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#d68c67{{ end }}",
            "{{ if gt .Ahead 0 }}#66a5ad{{ end }}",
            "{{ if gt .Behind 0 }}#b279a7{{ end }}"
          ],
          "options": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#3d3839>| </>{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#de6e7c> 󰤌 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#819b69>  {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#819b69",
          "options": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#bbbbbb",
          "style": "plain",
          "template": "$",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#b77e64",
          "options": {
            "style": "roundrock",
            "threshold": 500
          },
          "style": "plain",
          "template": " {{ .FormattedMs }}<#bbbbbb>  </>",
          "type": "executiontime"
        },
        {
          "foreground": "#819b69",
          "foreground_templates": ["{{ if gt .Code 0 }}#de6e7c{{ end }}"],
          "options": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "  {{ if gt .Code 0 }}{{ .Code }}{{ end }} ",
          "type": "status"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "final_space": true,
  "version": 4
}
