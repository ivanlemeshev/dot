{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "id": "User.copy"
    },
    {
      "command": "find",
      "id": "User.find"
    },
    {
      "command": "paste",
      "id": "User.paste"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "id": "User.splitPane"
    }
  ],
  "copyFormatting": "none",
  "copyOnSelect": true,
  "defaultProfile": "{c993c0b6-0023-5562-a928-3ea11eb283ce}",
  "keybindings": [
    {
      "id": "User.copy",
      "keys": "ctrl+shift+c"
    },
    {
      "id": "User.paste",
      "keys": "ctrl+shift+v"
    },
    {
      "id": "User.find",
      "keys": "ctrl+shift+f"
    },
    {
      "id": "User.splitPane",
      "keys": "alt+shift+d"
    }
  ],
  "newTabMenu": [
    {
      "type": "remainingProfiles"
    }
  ],
  "profiles": {
    "defaults": {
      "colorScheme": "Custom",
      "cursorShape": "filledBox",
      "font": {
        "builtinGlyphs": true,
        "cellHeight": "1.4",
        "colorGlyphs": true,
        "face": "Hack Nerd Font Mono",
        "size": 12,
        "weight": "normal"
      },
      "intenseTextStyle": "bright",
      "opacity": 100,
      "useAcrylic": false
    },
    "list": [
      {
        "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
        "guid": "{c993c0b6-0023-5562-a928-3ea11eb283ce}",
