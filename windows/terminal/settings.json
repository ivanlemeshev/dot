        "hidden": false,
        "icon": "üêö",
        "name": "Command Prompt",
        "startingDirectory": "%USERPROFILE%"
      },
      {
        "commandline": "wsl.exe -d Ubuntu --cd ~",
        "guid": "{e381fe40-9d15-58b7-99ea-5908cd6561e5}",
        "hidden": false,
        "icon": "üêß",
        "name": "Ubuntu"
      }
    ]
  },
  "schemes": [
    {
      "background": "#151515",
      "black": "#151515",
      "blue": "#6A9FB5",
      "brightBlack": "#505050",
      "brightBlue": "#6A9FB5",
      "brightCyan": "#75B5AA",
      "brightGreen": "#90A959",
      "brightPurple": "#AA759F",
      "brightRed": "#AC4142",
      "brightWhite": "#F5F5F5",
      "brightYellow": "#F4BF75",
      "cursorColor": "#D0D0D0",
      "cyan": "#75B5AA",
      "foreground": "#D0D0D0",
      "green": "#90A959",
      "name": "Custom",
      "purple": "#AA759F",
      "red": "#AC4142",
      "selectionBackground": "#505050",
      "white": "#D0D0D0",
      "yellow": "#F4BF75"
    }
  ],
  "themes": [
