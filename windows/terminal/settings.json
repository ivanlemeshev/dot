{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "id": "User.copy"
    },
    {
      "command": "find",
      "id": "User.find"
    },
    {
      "command": "paste",
      "id": "User.paste"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "id": "User.splitPane"
    }
  ],
  "copyFormatting": "none",
  "copyOnSelect": true,
  "defaultProfile": "{c993c0b6-0023-5562-a928-3ea11eb283ce}",
  "keybindings": [
    {
      "id": "User.copy",
      "keys": "ctrl+shift+c"
    },
    {
      "id": "User.paste",
      "keys": "ctrl+shift+v"
    },
    {
      "id": "User.find",
      "keys": "ctrl+shift+f"
    },
    {
      "id": "User.splitPane",
      "keys": "alt+shift+d"
    }
  ],
  "newTabMenu": [
    {
      "type": "remainingProfiles"
    }
  ],
  "profiles": {
    "defaults": {
      "colorScheme": "Custom",
      "cursorShape": "filledBox",
      "font": {
        "builtinGlyphs": true,
        "cellHeight": "1.4",
        "colorGlyphs": true,
        "face": "Hack Nerd Font Mono",
        "size": 12,
        "weight": "normal"
      },
      "intenseTextStyle": "bright",
      "opacity": 100,
      "useAcrylic": false
    },
    "list": [
      {
        "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
        "guid": "{c993c0b6-0023-5562-a928-3ea11eb283ce}",
        "hidden": false,
        "icon": "‚ö°",
        "name": "Windows PowerShell",
        "startingDirectory": "%USERPROFILE%"
      },
      {
        "commandline": "%SystemRoot%\\System32\\cmd.exe",
        "guid": "{0c63d969-ef2e-52d4-8736-68efbb78152d}",
        "hidden": false,
        "icon": "üêö",
        "name": "Command Prompt",
        "startingDirectory": "%USERPROFILE%"
      },
      {
        "commandline": "wsl.exe -d Ubuntu --cd ~",
        "guid": "{e381fe40-9d15-58b7-99ea-5908cd6561e5}",
        "hidden": false,
        "icon": "üêß",
        "name": "Ubuntu"
      }
    ]
  },
  "schemes": [
    {
      "background": "#191716",
      "black": "#0F0E0D",
      "blue": "#43454F",
      "brightBlack": "#383332",
      "brightBlue": "#65788F",
      "brightCyan": "#718062",
      "brightGreen": "#898471",
      "brightPurple": "#755E4A",
      "brightRed": "#8C4F4A",
      "brightWhite": "#BC9D66",
      "brightYellow": "#C8B491",
      "cursorColor": "#A18B62",
      "cyan": "#5C6652",
      "foreground": "#DABA8B",
      "green": "#57553C",
      "name": "Custom",
      "purple": "#604848",
      "red": "#845336",
      "selectionBackground": "#383332",
      "white": "#A18B62",
      "yellow": "#A17E3E"
    }
  ],
  "themes": [
    {
      "name": "Custom Dark",
      "tab": {
        "background": "#191716FF",
        "iconStyle": "default",
        "showCloseButton": "always",
        "unfocusedBackground": null
      },
      "tabRow": {
        "background": "#191716FF",
        "unfocusedBackground": "#191716FF"
      },
      "window": {
        "applicationTheme": "dark",
        "experimental.rainbowFrame": false,
        "frame": null,
        "unfocusedFrame": null,
        "useMica": false
      }
    }
  ]
}
